import(methods)
import(stats)
import(parallel)
importFrom("graphics", "abline", "axis", "layout", "lines", "mtext",
             "par", "plot", "points")

import(MASS)
import(mgcv)

export(LD, LD.formula, LD.LD,
       lethal.dose, lethal.dose.formula, lethal.dose.LD,
       confint, confint.LD,
       get_x)

S3method(lethal.dose, matrix)
S3method(LD, matrix)
S3method(lethal.dose, formula)
S3method(LD, formula)
S3method(lethal.dose, LD)
S3method(LD, LD)
S3method(lethal.dose, LDpred)
S3method(LD, LDpred)
S3method(print, LD)
S3method(plot, LD)
S3method(print, LDmatrix)
S3method(print, LDconfint)
S3method(plot, LDconfint)
S3method(summary, LD)
S3method(coef, LD)
S3method(predict, LD)
S3method(confint, LD)
S3method(lethal.dose, LD)
S3method(get_x, default)
S3method(get_x, LDpred)
